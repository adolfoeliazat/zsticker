        					<td height="4" bgcolor="#d50f25" width="6%" style="background-color:#d50f25 !important;line-height:4px; font-size:4px;"></td>
        					<td height="4" bgcolor="#eeb211" width="6%" style="background-color:#eeb211 !important;line-height:4px; font-size:4px;"></td>
        					<td height="4" bgcolor="#009925" width="6%" style="background-color:#009925 !important;line-height:4px; font-size:4px;"></td>
        					<td height="4" width="7%" style="line-height:4px; font-size:4px;"></td>
      					</tr></tbody>
